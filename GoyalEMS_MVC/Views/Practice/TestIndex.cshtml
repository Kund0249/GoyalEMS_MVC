
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div> 
        <p>Fruit List from ViewBag</p>
        <ol>
          @foreach (var item in ViewData["VDFruitList"])
          {
              <li>@item</li>
          }
        </ol>
    </div>
</body>
</html>
